


      <!-- Default box -->
      <div class="box box-warning">
        <div class="box-header">
          <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
              <i class="fa fa-minus"></i></button>
          </div>
        </div>
        <div class="box-body">
         
          <table id="tblAppCodes" class="table table-bordered table-striped" style="width:100%">
            <thead>
            <tr>
              <th>id</th>
              <th><@spring.message "program.appCodesController.appName" /></th>
              <th><@spring.message "program.appCodesController.codeType" /></th>
              <th><@spring.message "program.appCodesController.codeValue" /></th>
              <th><@spring.message "program.appCodesController.codeDesc" /></th>
              <th><@spring.message "program.appCodesController.codeDescEng" /></th>
              <th><@spring.message "label.ajaxOptions" /></th>
            </tr>
            </thead>
          </table>
                                             
        </div>
        <!-- /.box-body -->
      </div>
      <!-- /.box -->